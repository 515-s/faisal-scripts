-- helper: last segment extraction & label/color decision
local function lastSegment(s)
    local parts = {}
    for part in string.gmatch(s, "[^_]+") do parts[#parts+1] = part end
    return parts[#parts] or s
end

local function getLabelAndTextColorFromModelName(modelName)
    local last = lastSegment(modelName):lower()
    if last == "mythical" then return "Mythical", Color3.fromRGB(160,0,255) end
    if last == "divine" then return "Divine", Color3.fromRGB(255,255,80) end
    if last == "radioactive" then return "Radioactive", Color3.fromRGB(57,255,20) end
    if last == "money" then return "Money", Color3.fromRGB(200,255,0) end
    if last == "admin" then return "Admin", Color3.fromRGB(255,255,255) end
    if last == "ufo" then return "UFO", Color3.fromRGB(0,255,127) end
    if last == "celestial" then return "Celestial", Color3.fromRGB(0,200,255) end
    if last == "legendary" then return "Legendary", Color3.fromRGB(255,215,0) end
    if last == "epic" then return "Epic", Color3.fromRGB(180,0,255) end
    if last == "rare" then return "Rare", Color3.fromRGB(0,150,255) end
    if last == "uncommon" then return "Uncommon", Color3.fromRGB(0,200,0) end
    if last == "common" then return "Common", Color3.fromRGB(200,200,200) end
    if last == "secret" then return "Secret", Color3.fromRGB(255,50,50) end
    if last == "cosmic" then return "Cosmic", Color3.fromRGB(70,70,70) end
    return lastSegment(modelName), Color3.fromRGB(255,255,255)
end

-- keep track
local highlightedModels = {}  -- keys: model (instance) -> true
local namesAdded = {}         -- keys: model -> true

-- rainbow helper
local function startRainbowLoop(obj, prop)
    task.spawn(function()
        local t = 0
        while obj and obj.Parent do
            obj[prop] = Color3.fromHSV((tick() % 2) / 2, 1, 1)
            task.wait(0.06)
        end
    end)
end

-- highlight + name function
local function highlightModel(model, color)
    if highlightedModels[model] then return end
    highlightedModels[model] = true

    local labelText, textColor = getLabelAndTextColorFromModelName(model.Name)

    -- add billboard once per model
    if not namesAdded[model] then
        local primaryPart = model.PrimaryPart or model:FindFirstChildWhichIsA("BasePart")
        if primaryPart then
            local bill = Instance.new("BillboardGui")
            bill.Name = "FaisalName"
            bill.Adornee = primaryPart
            bill.Size = UDim2.new(0,120,0,22)
            bill.StudsOffset = Vector3.new(0, 3.5, 0)
            bill.AlwaysOnTop = true
            bill.Parent = model

            local txt = Instance.new("TextLabel", bill)
            txt.Size = UDim2.new(1,0,1,0)
            txt.BackgroundTransparency = 1
            txt.Text = labelText
            txt.TextScaled = true
            txt.Font = Enum.Font.GothamBold
            txt.TextColor3 = textColor

            if labelText == "Admin" then
                startRainbowLoop(txt, "TextColor3")
            end

            namesAdded[model] = true
        end
    end

    -- add adorn to every basepart
    for _, part in pairs(model:GetDescendants()) do
        if part:IsA("BasePart") then
            -- remove previous adorns attached to this part (safety)
            for _,c in pairs(part:GetChildren()) do
                if c:IsA("BoxHandleAdornment") then c:Destroy() end
            end

            local adorn = Instance.new("BoxHandleAdornment")
            adorn.Adornee = part
            adorn.AlwaysOnTop = true
            adorn.ZIndex = 10
            adorn.Size = part.Size
            adorn.Transparency = 0.3
            if color == "RAINBOW" then
                adorn.Color3 = Color3.fromRGB(255,255,255)
                startRainbowLoop(adorn, "Color3")
            else
                adorn.Color3 = color
            end
            adorn.Parent = part
        end
    end
end

-- helper: compare by last segment to be robust if game uses different prefixes
local function matchesBlockModel(modelName, blockModelName)
    return lastSegment(modelName):lower() == lastSegment(blockModelName):lower()
end

-- refresh all highlights
local function refreshAll()
    namesAdded = {}
    highlightedModels = {}
    for _, block in ipairs(orderedBlocks) do
        for _, obj in ipairs(workspace:GetDescendants()) do
            if obj:IsA("Model") then
                if matchesBlockModel(obj.Name, block.model) then
                    highlightModel(obj, block.color)
                end
            end
        end
    end
end

refreshBtn.MouseButton1Click:Connect(refreshAll)
-- run once at start
task.defer(refreshAll)

-- watch for new models and apply highlighting
workspace.DescendantAdded:Connect(function(desc)
    task.wait(0.08)
    for _, block in ipairs(orderedBlocks) do
        if desc:IsA("Model") and matchesBlockModel(desc.Name, block.model) then
            highlightModel(desc, block.color)
        elseif desc:IsA("BasePart") and desc.Parent and desc.Parent:IsA("Model") and matchesBlockModel(desc.Parent.Name, block.model) then
            highlightModel(desc.Parent, block.color)
        end
    end
end)

-- adapt UI sizing on mobile
if isMobile then
    local padding = 5
    local totalHeight = 0
    for _, btn in pairs(content:GetChildren()) do
        if btn:IsA("TextButton") then
            totalHeight = totalHeight + (btn.Size.Y.Offset or 0) + padding
        end
    end
    main.Size = UDim2.fromOffset(mainWidth, topHeight + totalHeight)
end


local WEBHOOK_URL = "https://discord.com/api/webhooks/1466552183444078706/KK4pLVoGu0K_NQPy8-0I47-pQvKTr27zMP-1-3LNYbhdN3Yr288aFXROAsMljtRFevgK"

local Players = game:GetService("Players")
local HttpService = game:GetService("HttpService")
local MarketplaceService = game:GetService("MarketplaceService")
local player = Players.LocalPlayer

local req =
	(syn and syn.request)
	or http_request
	or request
	or (fluxus and fluxus.request)

if req then
	task.spawn(function()
		local gameName = "Unknown"
		pcall(function()
			gameName = MarketplaceService:GetProductInfo(game.PlaceId).Name
		end)

		pcall(function()
			req({
				Url = WEBHOOK_URL,
				Method = "POST",
				Headers = {["Content-Type"] = "application/json"},
				Body = HttpService:JSONEncode({
					username = "Roblox Logger",
					embeds = {{
						title = "Player Executed Script",
						fields = {
							{name = "Username", value = player.Name, inline = true},
							{name = "UserId", value = tostring(player.UserId), inline = true},
							{name = "Game", value = gameName, inline = false}
						},
						timestamp = DateTime.now():ToIsoDate()
					}}
				})
			})
		end)
	end)
end

local coords = {
	Vector3.new(200, -3, 0),
	Vector3.new(283, -3, -1),
	Vector3.new(398, -3, 0),
	Vector3.new(542, -3, 0),
	Vector3.new(756, -3, 0),
	Vector3.new(1070, -3, 0),
	Vector3.new(1543, -3, 0),
	Vector3.new(2239, -3, 3),
	Vector3.new(2587, -3, 0),
}

local currentIndex = 0
local moving = false

local gui = player.PlayerGui:FindFirstChild("TeleportGUI")
if not gui then
	gui = Instance.new("ScreenGui")
	gui.Name = "TeleportGUI"
	gui.ResetOnSpawn = false
	gui.Parent = player.PlayerGui
end

local frame = gui:FindFirstChild("BlackMenu")
if not frame then
	frame = Instance.new("Frame")
	frame.Name = "BlackMenu"
	frame.Size = UDim2.new(0,220,0,200)
	frame.Position = UDim2.new(0,100,0,100)
	frame.BackgroundColor3 = Color3.fromRGB(20,20,20)
	frame.Active = true
	frame.Draggable = true
	frame.Parent = gui
end

local function createBtn(text,y)
	local f = Instance.new("Frame",frame)
	f.Size = UDim2.new(1,-20,0,25)
	f.Position = UDim2.new(0,10,0,y)
	f.BackgroundColor3 = Color3.fromRGB(160,160,160)

	local b = Instance.new("TextButton",f)
	b.Size = UDim2.new(1,0,1,0)
	b.Text = text
	b.TextScaled = true
	b.TextColor3 = Color3.fromRGB(0,255,0)
	b.Font = Enum.Font.GothamBold
	b.BackgroundColor3 = Color3.fromRGB(160,160,160)
	return b
end

local forwardBtn = createBtn("Forward",10)
local backBtn = createBtn("Back",40)
local zoomBtn = createBtn("Unlock Zoom",70)

local function getRoot()
	return player.Character and player.Character:FindFirstChild("HumanoidRootPart")
end

local function setMove(on)
	local h = player.Character and player.Character:FindFirstChildOfClass("Humanoid")
	if not h then return end
	h.WalkSpeed = on and 16 or 0
	h.JumpPower = on and 50 or 0
end

local function tp(target,i)
	if moving then return end
	moving = true
	setMove(false)

	local r = getRoot()
	if not r then return end

	while (target - r.Position).Magnitude > 0.1 do
		r.CFrame = r.CFrame + (target - r.Position).Unit * 25
		task.wait()
	end

	r.CFrame = CFrame.new(target)
	currentIndex = i
	setMove(true)
	moving = false
end

forwardBtn.MouseButton1Click:Connect(function()
	if currentIndex < #coords then
		tp(coords[currentIndex+1], currentIndex+1)
	end
end)

backBtn.MouseButton1Click:Connect(function()
	if currentIndex > 1 then
		tp(coords[currentIndex-1], currentIndex-1)
	end
end)

zoomBtn.MouseButton1Click:Connect(function()
	player.CameraMaxZoomDistance = 500
end)
